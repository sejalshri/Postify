<%-include('partials/head')-%>
<body>
    <nav class="navbar navbar-expand-lg">
        <div class="container-fluid">
            <button class="navbar-toggler dropbtn" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon Toggle"><i class="fas fa-bars"></i></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
                    <li class="nav-item navitem">
                        <a class="nav-link " href="/">Home</a>
                    </li>
                    <li class="nav-item navitem">
                        <a class="nav-link >" href="/<%=nav2h%>"><%=nav2%></a>
                    </li>
                    <li class="nav-item navitem">
                        <a class="nav-link " href="/<%=nav3h%>"><%=nav3%></a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container" style="color: white;">
        <% items.forEach(function(image) { %>
            <div>
                <div class="post">
                    <div class="name">
                        <h1><%= image.username %></h1>
                    </div>
                    <div class="date">
                        <small><%= image.date %></small>
                    </div><br>
                    <div class="title">
                        <h4><%= image.title %></h4>
                    </div>
                    <div class="description">
                        <p><%= image.description %></p>
                    </div>
                    <img src="data:image/<%=image.img.contentType%>;base64,
                    <%=image.img.data.toString('base64')%>" class="image_post">
                </div>
                <% }) %>
            </div>
        </div>
    </body>
    </html>
